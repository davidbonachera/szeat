         url('fonts/loveyalikeasister.woff') format('woff'),
         url('fonts/loveyalikeasister.ttf') format('truetype'),
         url('fonts/loveyalikeasister.svg#love_ya_like_a_sisterregular') format('svg');
    font-weight: normal;
    font-style: normal;
}
